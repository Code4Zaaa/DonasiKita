<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <Transition app:autoTransition="animateToEnd" app:constraintSetEnd="@+id/end" app:constraintSetStart="@+id/start" app:duration="1500" app:motionInterpolator="3">
        <KeyFrameSet>
            <KeyAttribute android:scaleX="1" android:scaleY="1" app:framePosition="0" app:motionTarget="@+id/imageViewLogo"/>
            <KeyAttribute android:scaleX="1.5" android:scaleY="1.5" app:framePosition="50" app:motionTarget="@+id/imageViewLogo"/>
            <KeyAttribute android:alpha="0" app:framePosition="50" app:motionTarget="@+id/imageViewQuotes"/>
            <KeyAttribute android:alpha="0" app:framePosition="50" app:motionTarget="@+id/textViewQuotes"/>
            <KeyPosition app:framePosition="50" app:keyPositionType="pathRelative" app:motionTarget="@+id/textViewQuotes" app:percentX="0" app:percentY="0"/>
            <KeyPosition app:framePosition="50" app:keyPositionType="pathRelative" app:motionTarget="@+id/imageViewLogo" app:percentX="0" app:percentY="0"/>
            <KeyAttribute android:scaleX="1.5" android:scaleY="1.5" app:framePosition="100" app:motionTarget="@+id/imageViewLogo"/>
        </KeyFrameSet>
    </Transition>
    <ConstraintSet android:id="@+id/start">
        <Constraint android:id="@+id/textViewQuotes" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginTop="100dp" android:alpha="0" android:layout_marginStart="40dp" android:layout_marginEnd="40dp" app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
        <Constraint android:id="@+id/imageViewQuotes" android:layout_width="53dp" android:layout_height="52dp" android:alpha="0" android:layout_marginStart="40dp" app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
        <Constraint android:id="@+id/imageViewLogo" android:layout_width="180dp" android:layout_height="180dp" app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end">
        <Constraint android:id="@+id/textViewQuotes" android:layout_width="0dp" android:layout_height="wrap_content" android:alpha="1" android:layout_marginStart="40dp" android:layout_marginEnd="40dp" app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
        <Constraint android:id="@+id/imageViewQuotes" android:layout_width="53dp" android:layout_height="52dp" android:alpha="1" android:layout_marginStart="40dp" app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
        <Constraint android:id="@+id/imageViewLogo" android:layout_width="180dp" android:layout_height="110dp" android:layout_marginBottom="12dp" app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"/>
    </ConstraintSet>
</MotionScene>
